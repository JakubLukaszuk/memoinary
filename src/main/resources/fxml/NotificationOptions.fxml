<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nihillon.controller.NotifiactionOptionController">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints />

    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="10.0" prefWidth="50.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="105.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints />
    </columnConstraints>

    <Label GridPane.columnIndex="1" GridPane.rowIndex="1">Display time</Label>
    <Label GridPane.columnIndex="1" GridPane.rowIndex="2">Notifcation interval</Label>
    <ChoiceBox GridPane.columnIndex="2" GridPane.rowIndex="1" fx:id="displayTimeComboBox"/>
    <ChoiceBox GridPane.columnIndex="2" GridPane.rowIndex="2" fx:id="intervalComboBox"/>
    <Button GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.rowSpan="2" onAction="#startNotification">Start</Button>
</GridPane>